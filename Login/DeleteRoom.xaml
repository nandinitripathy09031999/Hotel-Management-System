<Window x:Class="Login.DeleteRoom"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Login"
        mc:Ignorable="d"
        Title="DeleteRoom" Height="450" Width="800">
    <Grid Background="LightBlue">
        <TextBox x:Name="roomno" HorizontalAlignment="Left" Height="60" Margin="494,108,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="129" FontSize="24"/>
        <Button Content="Delete" Click="Button_Click" HorizontalAlignment="Left" Height="92" Margin="253,270,0,0" VerticalAlignment="Top" Width="260" FontWeight="Bold" FontSize="36"/>
        <Label Content="Enter Room no" HorizontalAlignment="Left" Height="84" Margin="83,96,0,0" VerticalAlignment="Top" Width="288" FontWeight="Bold" FontSize="36"/>

    </Grid>
</Window>
