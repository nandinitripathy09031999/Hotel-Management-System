<Window x:Class="Login.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Login"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="LightBlue">
        <Label Content="    USER LOGIN" HorizontalAlignment="Center" Height="55" Margin="0,44,0,0" VerticalAlignment="Top" Width="192" FontWeight="Bold" FontSize="22"/>
        <Button x:Name="logcust" Content="Login as Customer" HorizontalAlignment="Center" Height="69" Margin="0,131,0,0" VerticalAlignment="Top" Width="192" Click="Button_Click" FontWeight="Bold" FontSize="16"/>
        <Button x:Name="logemp" Content="Login as Employee"  HorizontalAlignment="Center" Height="69" Margin="0,241,0,0" VerticalAlignment="Top" Width="192" Click="Button_Click_1" FontWeight="Bold" FontSize="16"/>

    </Grid>
</Window>
