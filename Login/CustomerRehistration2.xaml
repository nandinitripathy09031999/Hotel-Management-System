<Window x:Class="Login.CustomerRehistration2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Login"
        mc:Ignorable="d"
        Title="CustomerRehistration2" Height="450" Width="800">
    <Grid Background="LightBlue">
        <Label Content="Name" HorizontalAlignment="Left" Height="40" Margin="38,120,0,0" VerticalAlignment="Top" Width="154" RenderTransformOrigin="0.033,0.464" FontWeight="Bold"/>
        <Label Content="Password" HorizontalAlignment="Left" Height="41" Margin="48,0,0,0" VerticalAlignment="Center" Width="134" RenderTransformOrigin="0.033,0.464" FontWeight="Bold"/>
        <Button Content="REGISTER" HorizontalAlignment="Left" Height="58" Margin="193,313,0,0" VerticalAlignment="Top" Width="190" FontWeight="Bold" FontSize="36" Click="Button_Click"/>
        <TextBox x:Name="cn" HorizontalAlignment="Left" Height="20" Margin="274,130,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="188"/>
        <TextBox x:Name="cp" HorizontalAlignment="Left" Height="21" Margin="274,217,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="188"/>


    </Grid>
</Window>
